                  </object>
                </child>
                <child>
                  <object class="GtkCheckButton" id="apply_gloss_effect_checkbutton">
                    <property name="label" translatable="yes">Glossy</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwActionRow">
            <property name="can_focus">0</property>
            <property name="title" translatable="yes">Use Dominant Color</property>
            <child>
              <object class="GtkSwitch" id="dominant_color_switch">
                <property name="valign">center</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwExpanderRow" id="grid1">
            <property name="can_focus">0</property>
            <property name="title" translatable="yes">Customize Indicator Style</property>
            <property name="expanded" bind-source="dot_style_switch" bind-property="active" bind-flags="sync-create|bidirectional"/>
            <child type="action">
              <object class="GtkSwitch" id="dot_style_switch">
                <property name="valign">center</property>
              </object>
            </child>
            <child>
              <object class="AdwActionRow" id="dot_color_box">
                <property name="can_focus">0</property>
                <property name="title" translatable="yes">Fill Color</property>
                <child>
                  <object class="GtkColorButton" id="dot_color_colorbutton">
                    <property name="valign">center</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow" id="border_color_box">
                <property name="can_focus">0</property>
                <property name="title" translatable="yes">Border Color</property>
                <child>
                  <object class="GtkColorButton" id="dot_border_color_colorbutton">
                    <property name="valign">center</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow" id="box_boder_width_box">
                <property name="can_focus">0</property>
                <property name="title" translatable="yes">Border Width</property>
                <child>
                  <object class="GtkSpinButton" id="dot_border_width_spin_button">
                    <property name="adjustment">dot_border_width_adjustment</property>
                    <property name="valign">center</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
